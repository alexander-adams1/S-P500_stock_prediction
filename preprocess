import pandas as pd
import csv
import tensorflow as tf
import numpy as np



def read_csv(filepath):
    dataframe = pd.read_csv(filepath)
    return dataframe

# def get_labels(dataframe):

def pos_or_neg(a):
    if a > 0:
        to_return = 1
    elif a < 0:
        to_return = -1
    else:
        to_return = 0
    return to_return

